#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sihmig/Projects/oss/ember-css-modules-poc/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sihmig/Projects/oss/ember-css-modules-poc/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ember-template-lint@4.18.2/node_modules/ember-template-lint/bin/ember-template-lint.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ember-template-lint@4.18.2/node_modules/ember-template-lint/bin/ember-template-lint.js" "$@"
fi
